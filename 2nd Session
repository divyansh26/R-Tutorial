whlile(z<5)
{
  z<z+2
  print(z)
}

v<-c("Hello","while loop")
cnt<-2
while(cnt<7){
  print(v)
  cnt=cnt+1
}

num<-1:5
for(val in num){
  if(val==3){
    break
  }
print(val)
}


num<-1:5
for(val in num){
  if(val==3){
 next
    }
  print(val)
}

