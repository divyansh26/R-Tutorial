new.function<-function(a)
  for(i in 1:a)
  {
    b<-i^2
    print(b)
  }

new.function(6)

myfirstfun<-function(n)
{
  n*n
}

k<-10
m<-myfirstfun(k)
m

myforthfun<-function(n,y=2)
{
  n^y 
}
myforthfun(2,3)


